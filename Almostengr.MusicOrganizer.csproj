    <AssemblyName>Almostengr.MusicOrganizer</AssemblyName>
    <RootNamespace>Almostengr.MusicOrganizer</RootNamespace>
    <Version>6.$([System.DateTime]::UtcNow.ToString(yy.MM.dd))</Version>
